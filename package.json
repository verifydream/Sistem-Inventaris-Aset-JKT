{
  "name": "sistem-inventaris-aset-jkt",
  "version": "1.0.0",
  "description": "Sistem Inventaris Aset JKT dengan fitur QR code, laporan PDF/Excel, dan rekomendasi AI",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm run install-all && npm run build"
  },
  "keywords": [
    "inventaris",
    "aset",
    "qrcode",
    "react",
    "express",
    "mongodb"
  ],
  "author": "JKT",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "exceljs": "^4.4.0",
    "pdfkit": "^0.14.0",
    "@google/generative-ai": "^0.1.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}